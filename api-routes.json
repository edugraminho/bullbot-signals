{
    "openapi": "3.1.0",
    "info": {
        "title": "BullBot Signals API",
        "description": "API para análise de RSI em criptomoedas usando múltiplas exchanges",
        "version": "1.0.0"
    },
    "paths": {
        "/rsi/single/{symbol}": {
            "get": {
                "tags": [
                    "RSI"
                ],
                "summary": "Get Rsi",
                "description": "Busca RSI para uma criptomoeda específica\n\nExemplos de símbolos: BTC, ETH, SOL",
                "operationId": "get_rsi_rsi_single__symbol__get",
                "parameters": [
                    {
                        "name": "symbol",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Symbol"
                        }
                    },
                    {
                        "name": "interval",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "description": "Intervalo: 1m, 3m, 5m, 15m, 30m, 1h, 2h, 4h, 6h, 8h, 12h, 1d, 3d, 1w, 1M",
                            "default": "1d",
                            "title": "Interval"
                        },
                        "description": "Intervalo: 1m, 3m, 5m, 15m, 30m, 1h, 2h, 4h, 6h, 8h, 12h, 1d, 3d, 1w, 1M"
                    },
                    {
                        "name": "window",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "description": "RSI calculation window",
                            "default": 14,
                            "title": "Window"
                        },
                        "description": "RSI calculation window"
                    },
                    {
                        "name": "source",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "description": "Fonte dos dados: binance, gate ou mexc",
                            "default": "binance",
                            "title": "Source"
                        },
                        "description": "Fonte dos dados: binance, gate ou mexc"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/RSIResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/rsi/multiple": {
            "get": {
                "tags": [
