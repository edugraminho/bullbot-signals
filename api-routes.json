                "type": "object",
                "required": [
                    "symbol",
                    "rsi_value",
                    "current_price",
                    "timestamp",
                    "timespan",
                    "window",
                    "source",
                    "data_source"
                ],
                "title": "RSIResponse",
                "description": "DTO para resposta de consulta de RSI"
            },
            "SignalResponse": {
                "properties": {
                    "id": {
                        "type": "integer",
                        "title": "Id"
                    },
                    "symbol": {
                        "type": "string",
                        "title": "Symbol"
                    },
                    "signal_type": {
                        "type": "string",
                        "title": "Signal Type"
                    },
                    "strength": {
                        "type": "string",
                        "title": "Strength"
                    },
                    "price": {
                        "type": "number",
                        "title": "Price"
                    },
                    "timeframe": {
                        "type": "string",
                        "title": "Timeframe"
                    },
                    "source": {
                        "type": "string",
                        "title": "Source"
                    },
                    "message": {
                        "type": "string",
                        "title": "Message"
                    },
                    "indicator_type": {
                        "items": {
                            "type": "string"
                        },
                        "type": "array",
                        "title": "Indicator Type"
                    },
                    "indicator_data": {
                        "type": "object",
                        "title": "Indicator Data"
                    },
                    "confidence_score": {
                        "anyOf": [
                            {
                                "type": "number"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Confidence Score"
                    },
                    "combined_score": {
                        "anyOf": [
                            {
                                "type": "number"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Combined Score"
                    },
                    "processed": {
                        "type": "boolean",
                        "title": "Processed"
                    },
                    "processed_at": {
                        "anyOf": [
                            {
                                "type": "string",
                                "format": "date-time"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Processed At"
                    },
                    "processed_by": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Processed By"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Created At"
                    }
                },
                "type": "object",
                "required": [
                    "id",
                    "symbol",
                    "signal_type",
                    "strength",
                    "price",
                    "timeframe",
                    "source",
                    "message",
                    "indicator_type",
                    "indicator_data",
                    "confidence_score",
                    "combined_score",
                    "processed",
                    "processed_at",
                    "processed_by",
                    "created_at"
                ],
                "title": "SignalResponse",
                "description": "Schema de resposta para sinais"
            },
            "SystemStatusResponse": {
                "properties": {
                    "monitoring_configs": {
                        "type": "integer",
                        "title": "Monitoring Configs"
                    },
                    "active_configs": {
                        "type": "integer",
                        "title": "Active Configs"
                    },
                    "last_signal_count_24h": {
                        "type": "integer",
                        "title": "Last Signal Count 24H"
                    },
                    "unprocessed_signals": {
                        "type": "integer",
                        "title": "Unprocessed Signals"
                    },
                    "celery_workers_active": {
                        "type": "boolean",
                        "title": "Celery Workers Active"
                    }
                },
                "type": "object",
                "required": [
                    "monitoring_configs",
                    "active_configs",
                    "last_signal_count_24h",
                    "unprocessed_signals",
                    "celery_workers_active"
