                    },
                    "timestamp": {
                        "type": "string",
                        "title": "Timestamp"
                    },
                    "timespan": {
                        "type": "string",
                        "title": "Timespan"
                    },
                    "window": {
                        "type": "integer",
                        "title": "Window"
                    },
                    "source": {
                        "type": "string",
                        "title": "Source"
                    },
                    "data_source": {
                        "type": "string",
                        "title": "Data Source"
                    }
