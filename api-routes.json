    "components": {
        "schemas": {
            "CeleryStatusResponse": {
                "properties": {
                    "active_workers": {
                        "type": "integer",
                        "title": "Active Workers"
                    },
                    "scheduled_tasks": {
                        "type": "integer",
                        "title": "Scheduled Tasks"
                    },
                    "failed_tasks": {
                        "type": "integer",
                        "title": "Failed Tasks"
                    },
                    "monitoring_task_last_run": {
                        "anyOf": [
                            {
                                "type": "string",
                                "format": "date-time"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Monitoring Task Last Run"
                    },
                    "monitoring_task_next_run": {
                        "anyOf": [
                            {
                                "type": "string",
                                "format": "date-time"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Monitoring Task Next Run"
                    }
                },
                "type": "object",
                "required": [
                    "active_workers",
                    "scheduled_tasks",
                    "failed_tasks",
                    "monitoring_task_last_run",
                    "monitoring_task_next_run"
                ],
                "title": "CeleryStatusResponse",
                "description": "Schema de resposta para status do Celery"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "HealthResponse": {
                "properties": {
                    "status": {
                        "type": "string",
                        "title": "Status"
                    },
