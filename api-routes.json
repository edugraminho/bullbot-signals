                "summary": "Get Multiple Rsi",
                "description": "Busca RSI para múltiplas criptomoedas\n\nExemplo: /rsi/multiple?symbols=BTC,ETH,SOL",
                "operationId": "get_multiple_rsi_rsi_multiple_get",
                "parameters": [
                    {
                        "name": "symbols",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "description": "Símbolos separados por vírgula (ex: BTC,ETH,SOL)",
                            "title": "Symbols"
                        },
                        "description": "Símbolos separados por vírgula (ex: BTC,ETH,SOL)"
                    },
                    {
                        "name": "interval",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "description": "Intervalo: 1m, 3m, 5m, 15m, 30m, 1h, 2h, 4h, 6h, 8h, 12h, 1d, 3d, 1w, 1M",
                            "default": "1d",
                            "title": "Interval"
                        },
                        "description": "Intervalo: 1m, 3m, 5m, 15m, 30m, 1h, 2h, 4h, 6h, 8h, 12h, 1d, 3d, 1w, 1M"
                    },
                    {
                        "name": "window",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "description": "RSI calculation window",
                            "default": 14,
                            "title": "Window"
                        },
                        "description": "RSI calculation window"
                    },
                    {
                        "name": "source",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "description": "Fonte dos dados: binance, gate ou mexc",
                            "default": "binance",
                            "title": "Source"
                        },
                        "description": "Fonte dos dados: binance, gate ou mexc"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/MultipleRSIResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
